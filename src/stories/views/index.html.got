<article>
<ul class="stories">
  {{ $0 := . }}
  {{ range .stories }}
     {{ set $0 "story" . }}
     {{ template "stories/views/row.html.got" $0 }}
  {{ end }}
  {{ if eq (len .stories) 100 }}
  <li class="more_link story"><a href="?page={{add .page 1 }}">显示更多</a></li>
  {{ end }}
</ul>
</article>