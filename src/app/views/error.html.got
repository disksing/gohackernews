<section class="padded">
      <h1>{{.title}}</h1>
      <p>{{.message}}</p>
      {{ if .file }}
      <p>File:{{.file}}</p>
      {{ end }}
      {{ if .error }}
       <pre><code>
      Error:{{.error}}
      </code></pre>
      {{ end }}
      {{ if .current_user.Anon }}
      <p><a href="/users/login" class="button">登入</a></p>   
      {{ end }}
      <p><a class="button grey" method="back">返回</a></p>   
</section>